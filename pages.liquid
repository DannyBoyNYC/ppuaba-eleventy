---
pagination:
  data: contentful
  size: 1
  alias: page
  addAllPagesToCollections: true
permalink: "pages/{{ page.slug | slug }}/"
tags: "page"
layout: pages.liquid
eleventyComputed:
  title: "{{ page.title}}"
---

<h1>
  {{ page.title }}
</h1>

<time>{{ page.date | date : "%Y-%m-%d" }}</time>

<img
  class="u-max-full-width"
  src="{{ page.imageUrl }}"
  alt="{{ page.title }}"
  loading="lazy"
/>

{{ page.body }}
